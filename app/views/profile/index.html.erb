<div class='row'>
  <div class="twelve columns">
    <h1>Profile</h1>
    <ul>
      <li><%= current_user.username %></li>
      <li><%= current_user.email %></li>
    </ul>
  </div>
</div>
<div class='row'>
  <div class="twelve columns">
    <h1>Reviews</h1>
    <%- @reviews.each do |review| %>
      <%- if review.id %>
        <p><%= review.inspect %></p>
      <% end %>
    <% end %>
  </div>
</div>
